<header>
  <h1><%= pluralize(@entries.size, 'Entry') %></h1>
</header>

  <% @entries.each do |entry| %>
  <ul>
    <li>
      Name: <%= entry.name %>
    </li>
    <li>
      Year: <%= entry.year%>
    </li>
    <li>
      Selection: <%= entry.selection.title %>
    </li>
    <li>
      Student: <%= entry.student.name %>
    </li>
  </ul>

  <% end %>

  <footer>
    <%= link_to 'Home Page', root_path %>
  </footer>
